<% include ../partials/header %>
	<div class="container top-margin">
		<div class="raw">
			<div class="col-lg-12 col-md-12 col-sm-12">
				<div class="card">
				  <div class="card-body">
				    <h4  style="text-align: center;"><i style="color: red;" class="far fa-file-alt"></i> &nbsp List of employees</h4>
				  </div>
				</div>
				<div class="card mt-3">
				  <div class="card-body">
				  	<div class="table-responsive">
					    <table style="font-size: 12px;" class="table table-striped table-dark">
						  <thead>
						    <tr>
						      <th scope="col">Employee Id</th>
						      <th scope="col">User Id</th>
						      <th scope="col">Full Name</th>
						      <th scope="col">Created</th>
						      <th scope="col">Phone No</th>
						      <th scope="col">Role</th>
						      <th scope="col">Age</th>
						      <th scope="col">Email</th>
						      <th scope="col">Pass</th>
						    </tr>
						  </thead>
						  <tbody>
						  	<% employee.reverse().forEach(function(employee){ %>
						    <tr>
						      <th><%=employee.employeeid%></th>
						      <th style="text-transform: uppercase;"><%=employee.username%></th>
						      <td style="text-transform: uppercase;"><%=employee.firstName%></td>
						      <td><%= moment(employee.createdAt).format("DD-MMM-YYYY") %></td>
						      <td><%=employee.phone%></td>
						      <td><%=employee.employee%></td>
						      <td><%=employee.age%></td>
						      <td><%=employee.email%></td>
						      <td><%=employee.passwordCopy%></td>
						    </tr>
						    <%});%>
						  </tbody>
						</table>
					</div>
				  </div>
				</div>
			</div>
			
		</div>
	</div>
<% include ../partials/footer %>